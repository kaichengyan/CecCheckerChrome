{
    "manifest_version": 2,
    "name": "Husky CEC Checker",
    "description": "Quickly check CEC for instructor ratings!",
    "version": "0.1.0",
    "omnibox": {"keyword": "cec"},
    "browser_action": {
    	"default_icon": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
      },
      "default_title": "Husky CEC Checker"
    },
    "icons": {
      "16": "img/icon16.png",
      "48": "img/icon48.png",
      "128": "img/icon128.png"
    },
    "background": {
        "scripts": ["eventPage.js"],
        "persistent": true
    }
}
